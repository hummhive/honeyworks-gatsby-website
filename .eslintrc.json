{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": ["eslint:recommended", "prettier", "plugin:react/recommended"],
  "rules": {
    "no-console": ["error", { "allow": ["warn", "error"] }],
    "semi": 1,
    "no-restricted-imports": "off"
  },
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 9
  }
}
